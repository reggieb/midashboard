<h1>Users</h1>
<p><%= link_to 'Create user', new_user_path %></p>
<ul>
<% @users.each do |user| %>
  <li>
    <%= user.email %>
    <%= link_to 'Edit', edit_user_path(user) %>
    <%= link_to 'Show', user %>
    <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %>
  </li>
<% end %>
</ul>
