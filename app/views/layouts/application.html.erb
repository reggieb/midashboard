<!DOCTYPE html>
<html>
<head>
  <title>Dashboard</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <script src='https://www.google.com/jsapi'></script>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
 
  <div class="container">
    <%= content_tag('p', login_link, class: 'login') unless current_page?(new_user_session_path) %>
    <%= content_tag('p', notice, class: 'notice') if notice %>
    <%= content_tag('p', alert, class: 'alert') if alert %>

    <%= render 'admin_nav' if current_user.try('admin?') %>

    <%= yield %>
    
  </div>

</body>
</html>
